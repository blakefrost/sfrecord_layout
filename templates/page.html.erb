<div class="broadsheet-page" <%= "data-page-id=#{page.id}" if composing? %> style="height: <%= page.pixel_height %>px">
  <% if page.number == 1 %>
    <%= render "mastheads/#{template_name}" %>
  <% else -%>
    <%= render 'page_header', page: page %>
  <% end %>

  <% page.content_items.each do |content_item| %>
    <%= render_content_item content_item %>
  <% end -%>

</div>
