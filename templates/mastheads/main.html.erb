<!-- Masthead -->
<div class="row">
  <div class="col span24">
    <div class="masthead">
      <%= render 'logo' -%>
      <div class="masthead-foot">
        <p class="volume pull-left"><%= edition.volume_label %></p>
        <p class="price pull-right">FREE</p>
        <p class="publish-date"><%= edition.try(:publish_date).try(:strftime, "%A, %B %d, %Y") %></p>
      </div>
    </div>

  </div>
</div>
